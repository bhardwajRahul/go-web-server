# PostgreSQL Configuration
DATABASE_URL=postgres://your_username:your_password@localhost:5432/your_database?sslmode=disable

# Alternative individual settings
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=gowebserver
DATABASE_USER=your_username
DATABASE_PASSWORD=your_password
DATABASE_SSLMODE=disable

# Server Configuration
SERVER_PORT=8080
SERVER_HOST=

# Application Configuration
APP_ENVIRONMENT=development
APP_DEBUG=false
APP_LOG_LEVEL=info
APP_LOG_FORMAT=text

# Security Configuration
SECURITY_ENABLE_CORS=true
SECURITY_ALLOWED_ORIGINS=*

# Feature Flags
FEATURES_ENABLE_METRICS=false
FEATURES_ENABLE_PPROF=false